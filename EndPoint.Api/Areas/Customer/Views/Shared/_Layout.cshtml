<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="rtl">
<!-- BEGIN: Head-->

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="PIXINVENT">
    <title>داشبورد مشتری | @ViewBag.Title </title>
    <link rel="apple-touch-icon" href="~/lib/Content/app-assets//images/icons.\/jpg.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/app-assets/images/ico/favicon.ico">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600" rel="stylesheet">
    <link href="~/Content/app-assets/fonts/font-googel.css" rel="stylesheet">


    <!-- BEGIN: Vendor CSS-->

    <link rel="stylesheet" type="text/css" href="~/content/app-assets/vendors/css/vendors-rtl.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/vendors/css/charts/apexcharts.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/vendors/css/extensions/tether-theme-arrows.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/vendors/css/extensions/tether.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/vendors/css/extensions/shepherd-theme-default.css">
    <!-- END: Vendor CSS-->
    <!-- BEGIN: Theme CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/colors.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/components.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/themes/dark-layout.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/themes/semi-dark-layout.css">

    <!-- BEGIN: Page CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/core/menu/menu-types/vertical-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/core/colors/palette-gradient.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/pages/dashboard-analytics.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/pages/card-analytics.css">
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/plugins/tour/tour.css">


    <!-- END: Page CSS-->
    <!-- BEGIN: Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/custom-rtl.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css/style-rtl.css">

    <!-- END: Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/app-assets/css-rtl/plugins/forms/validation/form-validation.css">
</head>
<!-- END: Head-->
<!-- BEGIN: Body-->
<body class="vertical-layout vertical-menu-modern 2-columns  navbar-floating footer-static  " data-open="click" data-menu="vertical-menu-modern" data-col="2-columns">

    <!-- BEGIN: Header-->
    @await Html.PartialAsync("_Header")
    <!-- END: Header-->
    <!-- BEGIN: Main Menu-->
    @await Html.PartialAsync("_Menu")
    <!-- END: Main Menu-->
    <!-- BEGIN: Content-->
    <div class="app-content content">
        @RenderBody()
    </div>
    <!-- END: Content-->

    <div class="sidenav-overlay"></div>
    <div class="drag-target"></div>

    <!-- BEGIN: Footer-->
    <footer class="footer footer-static footer-light">
        <p class="clearfix blue-grey lighten-2 mb-0">
            <span class="float-md-left d-block d-md-inline-block mt-25">شرکت &copy; @DateTime.Now.Year<a class="text-bold-800 grey darken-2" href="#" target="_blank">پارس کیان,</a>تمامی حقوق محفوظ است</span><span class="float-md-right d-none d-md-block"><i class="feather icon-heart pink"></i></span>
            <button class="btn btn-primary btn-icon scroll-top" type="button"><i class="feather icon-arrow-up"></i></button>
        </p>
    </footer>
    <!-- END: Footer-->
    <!-- BEGIN: Vendor JS-->
    <script src="~/Content/app-assets/vendors/js/vendors.min.js"></script>
    <!-- BEGIN Vendor JS-->
    <!-- BEGIN: Page Vendor JS-->
    <script src="~/Content/app-assets/vendors/js/charts/apexcharts.min.js"></script>
    <script src="~/Content/app-assets/vendors/js/extensions/tether.min.js"></script>
    <script src="~/Content/app-assets/vendors/js/extensions/shepherd.min.js"></script>
    <!-- END: Page Vendor JS-->
    <!-- BEGIN: Theme JS-->
    <script src="~/Content/app-assets/js/core/app-menu.js"></script>
    <script src="~/Content/app-assets/js/core/app.js"></script>
    <script src="~/Content/app-assets/js/scripts/components.js"></script>
    <!-- END: Theme JS-->
    <!-- BEGIN: Page JS-->
    <script src="~/Content/app-assets/js/scripts/pages/dashboard-analytics.js"></script>
    <!-- BEGIN: Vendor JS-->
    <script src="~/Content/app-assets/vendors/js/vendors.min.js"></script>
    <!-- BEGIN Vendor JS-->
    <!-- BEGIN: Page Vendor JS-->
    <script src="~/Content/app-assets/vendors/js/forms/validation/jqBootstrapValidation.js"></script>
    <!-- END: Page Vendor JS-->
    <!-- BEGIN: Page JS-->
    <script src="~/Content/app-assets/js/scripts/forms/validation/form-validation.js"></script>
    <script src="~/Content/Sweet/sweetalert2.all.min.js"></script>
    <script src="~/Content/Sweet/sweet-alerts.js"></script>
    <!-- END: Page JS-->
    <script src="~/js/script.min.js"></script>
</body>
<!-- END: Body-->

</html>