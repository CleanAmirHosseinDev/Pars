    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>

    <script>
        Web.Corporate.InitCorporate(@ViewBag.id, false);
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
